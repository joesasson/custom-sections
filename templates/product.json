            "custom_liquid": "<div class=\"product-form__input product-form__input--dropdown\">\n<label class=\"form__label\">Camp Name<\/label>\n<div class=\"select\">\n<select class=\"select__select\" id=\"camp-name\" name=\"properties[Camp Name]\" form=\"{{ 'product-form-' | append: section.id }}\" required>\n    <option value=\"\" hidden selected>Please select<\/option>\n        <option value=\"Option1\">Option1<\/option>\n        <option value=\"Option2\">Option2<\/option>\n        <option value=\"Option3\">Option3<\/option>\n<\/select>\n<svg aria-hidden=\"true\" focusable=\"false\" class=\"icon icon-caret\" viewBox=\"0 0 10 6\">\n<path fill-rule=\"evenodd\" clip-rule=\"evenodd\" \nd=\"M9.354.646a.5.5 0 00-.708 0L5 4.293 1.354.646a.5.5 0 00-.708.708l4 4a.5.5 0 00.708 0l4-4a.5.5 0 000-.708z\" \nfill=\"currentColor\">\n<\/path><\/svg>\n<\/div><\/div>\n    <script>\n    document.addEventListener(\"DOMContentLoaded\", ()=>{document.querySelector(\"form[novalidate]\").removeAttribute(\"novalidate\")})\n    <\/script>"
